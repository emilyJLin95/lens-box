  "devDependencies": {
    "dotenv": "10.0.0",
    "ethers": "5.5.1",
    "truffle-plugin-verify": "^0.6.1"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.8.1",
    "@truffle/hdwallet-provider": "^2.1.5"
  },
