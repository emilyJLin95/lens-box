{
  "name": "lens-truffle-box",
  "version": "1.0.0",
  "description": "Composable and decentralized social graph",
  "scripts": {
    "test": "truffle test",
    "deploy-local": "truffle migrate --skip-dry-run",
    "deploy-mumbai": "truffle migrate --skip-dry-run --network mumbai",
    "deploy-dashboard": "truffle migrate --skip-dry-run --network dashboard",
    "ganache:goerli": "ganache --fork goerli",
    "ganache:mainnet": "ganache --fork mainnet",
    "compile": "truffle compile",
    "console:dashboard": "truffle console --network dashboard",
    "verify": "truffle run verify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emilyjlin95/lens-box"
  },
  "bugs": {
    "url": "https://github.com/aave/lens-protocol/issues"
  },
